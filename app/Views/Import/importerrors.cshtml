
    <ul class="importError">
        @{
            Dictionary<string, List<string>> errors = new Dictionary<string, List<string>>(ViewBag.errors);
        }
        @foreach (var error in errors.OrderBy(x => x.Key))
        {
            if (error.Key != "STRUCTURE") continue;
            <li class="importPiecesKo importError">
                Erreur de Structure
                <ul class="importError">
                    @foreach (var errorLignes in error.Value)
                    {
                        <li> @errorLignes</li>
                    }
                </ul>
            </li>
        }
        @foreach (var error in errors.OrderBy(x => x.Key))
        {
            if (error.Key == "STRUCTURE") continue;
            <li class="importPiecesKo importError">
                Pièce : @error.Key
                <ul class="importError">
                    @foreach (var errorLignes in error.Value)
                    {
                        <li> @errorLignes</li>
                    }
                </ul>
            </li>
        }
    </ul>
